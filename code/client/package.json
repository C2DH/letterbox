{
  "name": "@letterbox/client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist .eslintcache",
    "generate": "graphql-codegen",
    "build": "tsc -b && vite build",
    "dev": "concurrently -n dev,generate \"vite --host\" \"npm run generate -- --watch\"",
    "start": "npm run build && vite preview --host",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
    "compile": "tsc -b"
  },
  "dependencies": {
    "@apollo/client": "^3.13.4",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@ouestware/facets": "^0.0.1-alpha.5",
    "@ouestware/facets-client": "^0.0.1-alpha.15",
    "@ouestware/hooks": "^0.0.1-alpha.4",
    "@ouestware/infinite-scroll": "0.0.1-alpha.8",
    "@ouestware/loaders": "^0.0.1-alpha.5",
    "@ouestware/modals": "^0.0.1-alpha.5",
    "@ouestware/notifications": "^0.0.1-alpha.5",
    "@ouestware/tooltip": "^0.0.1-alpha.6",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "7.3.0",
    "react-router-typesafe": "^1.5.0",
    "react-select": "^5.10.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "sass": "^1.79.4",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-checker": "^0.9.0"
  }
}
