{
  "extends": "../tsconfig.base.json",
  "include": ["src"],
  "compilerOptions": {
    "declaration": false,
    "declarationDir": null,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["esnext"],
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "outDir": "build",
    "rootDir": "src",
    "strictPropertyInitialization": false
  }
}
